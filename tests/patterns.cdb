
# Test 1 - 3: Range wildcards.

@reset.
{m,n,p,q,r}::Indices(vector).
A_{ #{m,1..3} }::AntiSymmetric.
obj1:= A_{m n p p};
@canonicalise!(%);
tst1:= A_{m n p p} - @(obj1);
@collect_terms!(%);
@assert(tst1);

obj2:= A_{m p p};
@canonicalise!(%);
@collect_terms!(%);
@assert(obj2);

obj3:= A_{\mu\nu\nu};
@canonicalise!(%);
tst3:= A_{\mu\nu\nu} - @(obj3);
@collect_terms!(%);
@assert(tst3);
